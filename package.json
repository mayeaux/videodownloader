{
  "name": "videodownloader",
  "version": "1.0.0",
  "description": "A video and channel downloader that supports 110 websites, powered by Electron and youtube-dl",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "build": "build"
  },
  "build": {
    "asar": false,
    "appId": "yourappid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "repository": "https://github.com/mayeaux/videodownloader",
  "keywords": [
    "Electron",
    "youtube-dl"
  ],
  "author": "Mayeaux",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.19",
    "fs-extra": "^8.1.0",
    "youtube-dl": "https://github.com/mayeaux/workingyoutubedl",
    "ytdl-core": "^0.29.2"
  }
}
